{
  "name": "llm-univ",
  "version": "0.0.1",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "RUN_INTEGRATION_TESTS=true jest --testPathPattern=integration",
    "test:all": "RUN_INTEGRATION_TESTS=true jest",
    "test:github": "RUN_INTEGRATION_TESTS=true jest --testNamePattern=\"GitHub Models\"",
    "test:coverage": "jest --coverage",
    "test:gh": "RUN_PROVIDER=github jest",
    "test:openai": "RUN_PROVIDER=openai jest",
    "test:anthropic": "RUN_PROVIDER=anthropic jest",
    "test:all-providers": "RUN_ALL_PROVIDERS=true jest",
    "collect:gh": "node src/provider_info_collector.js --provider=github",
    "collect:all": "node src/provider_info_collector.js --all",
    "test:full": "npm run test:unit && npm run test:integration && npm run test:all-providers",
    "test:ci": "npm run test:coverage && npm run test:full"
  },
  "keywords": [
    "llm",
    "ai",
    "openai",
    "anthropic",
    "google",
    "gemini",
    "claude",
    "gpt",
    "chatgpt",
    "universal",
    "api",
    "machine-learning",
    "natural-language-processing"
  ],
  "author": "",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "testMatch": ["**/__tests__/**/*.test.js", "**/?(*.)+(spec|test).js"]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "zod": "^3.25.36"
  }
}
